Proceso ejercicio8cap5
	Definir num como entero;
	
    escribir "digite la cantidad de años que desea consultar";
	leer num;
	
	segun num hacer
		10: escribir "bodas de hojalata";
		20: escribir "bodas de porcelana";
		30: escribir "bodas de perlas";
		40: escribir "bodas de rubi";
		50: escribir "bodas de oro";
		60: escribir "bodas de diamente";
		de otro modo
			escribir "no existe boda para la cantidad de años ingresado";
			
	FinSegun
	
FinProceso
